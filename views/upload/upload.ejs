<%- include ./../header%>
<div class="body">
	<%- include ./../nav%>
	<br>
	<div class="container">
		<div class="col-md-4 col-md-offset-5">上传test</div>
		<br>
		<div id="post" class="col-md-10 col-md-offset-1">
			<form action="/do_upload_test?_csrf=<%= csrf%>" method="post" enctype="multipart/form-data">

				<div class="form-group">
					<label for="name">文件名:</label>
					<input type="hidden" name="_csrf" value="<%= csrf%>">
					<input class="form-control" name="name" type="text">
				</div>

				<div class="form-group">
					<label for="password">File:</label>
					<input name="image" type="file">
				</div>
				<div class="form-group">
					<input class="btn btn-success form-control" type="submit" value="提交">
				</div>
			</form>
		</div>
	</div>
</div>
<%- include ./../bottom%>